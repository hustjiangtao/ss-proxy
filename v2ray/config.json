{
    "log": {
        "access": "/var/log/v2ray-access.log",
        "error": "/var/log/v2ray-error.log",
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 8001,
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "<uuid>",
                        "level": 1,
                        "alterId": 0
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/ws"
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {}
        }
    ]
}